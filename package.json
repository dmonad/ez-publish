{
  "name": "ez-publish",
  "version": "0.0.0",
  "description": "Command line tool for publishing js libraries",
  "main": "ez-publish.js",
  "scripts": {
    "dist": "rollup -c",
    "watch": "rollup -cw",
    "lint": "standard",
    "debug": "npm run dist && cd _debugging && node --inspect --debug-brk ../ez-publish.js"
  },
  "keywords": [
    "publish",
    "release"
  ],
  "author": "Kevin Jahns <kevin.jahns@rwth-aachen.de>",
  "license": "MIT",
  "devDependencies": {
    "rollup": "^0.41.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-json": "^2.1.0",
    "rollup-watch": "^3.2.2",
    "standard": "^8.6.0"
  },
  "dependencies": {
    "commander": "^2.9.0",
    "ez-async": "^1.0.0-alpha.1",
    "load-json-file": "^2.0.0",
    "semver": "^5.3.0",
    "write-json-file": "^2.0.0"
  }
}
